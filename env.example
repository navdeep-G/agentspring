# Enterprise Agent API Environment Configuration
# Copy this file to .env and modify as needed

# API Configuration
CUSTOMER_SUPPORT_AGENT_API_KEY=demo-key
ADMIN_API_KEY=admin-key

# Celery Configuration
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0

# Ollama Configuration
OLLAMA_BASE_URL=http://ollama:11434

# Logging Configuration
LOG_LEVEL=INFO

# Optional: Custom API Key (overrides default)
# CUSTOMER_SUPPORT_AGENT_API_KEY=your-custom-api-key

# Optional: Custom Admin Key (overrides default)
# ADMIN_API_KEY=your-custom-admin-key 